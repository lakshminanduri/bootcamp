  "scripts": {
    "build": "nx build",
    "build:affected": "nx affected:build --base=origin/${CHANGE_TARGET:-main}",
    "docker:build": "nx affected --base=origin/${CHANGE_TARGET:-main} --target=build-docker --docker-registry=${CHANGE_DOCKER_REGISTRY:-harbor.backbase.eu/staging} --image-tag=${CHANGE_IMAGE_TAG:-latest}",
    "docker:build:all": "nx affected --all --target=build-docker",
    "docker:push": "nx affected --base=origin/${CHANGE_TARGET:-main} --target=push-docker --docker-registry=${CHANGE_DOCKER_REGISTRY:-harbor.backbase.eu/staging} --image-tag=${CHANGE_IMAGE_TAG:-latest}",
    "e2e": "nx affected:e2e --base=origin/${CHANGE_TARGET:-main}",
    "e2e:watch": "npm run e2e -- --watch",
    "e2e:retail-usa": "nx run retail-usa-e2e:e2e",
    "format": "nx format:write --parallel",
    "format:check": "nx format:check --parallel --base=origin/${CHANGE_TARGET:-main}",
    "format:write": "nx format:write --parallel",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "lint": "nx workspace-lint && npm run lint:affected",
    "lint:affected": "nx affected:lint --base=origin/${CHANGE_TARGET:-main}",
    "lint:all": "nx run-many --all --target=lint",
    "mock-server": "node ./mock-server/mock-server.js",
    "ng": "nx",
    "start": "nx serve",
    "start:mocks": "nx serve -c mocks",
    "test": "nx run-many --all --target=test",
    "test:affected": "nx affected:test --base=origin/${CHANGE_TARGET:-main}",
    "xi18n:retail-usa": "ng extract-i18n retail-usa --output-path=apps/retail-usa/src/locale && xliffmerge --profile=apps/retail-usa/src/xliffmerge.json en es"
  },
